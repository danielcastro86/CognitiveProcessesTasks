**************************************************************************************************************
**************************************************************************************************************
	BLOCKS
**************************************************************************************************************
**************************************************************************************************************

<block practice>
/ preinstructions = (intro)
/ trials = [1-3 = trial.practice_scenario]
/ postinstructions = (test)
</block>

<block test>
/ onblockbegin = [
	values.blockCount += 1;
]
/ trials = [
	1 = testBlockStart;
	2-14 = noreplace(trial.probe_scenario, trial.nprobe_scenario,
trial.training_scenario, trial.training_scenario, trial.training_scenario, trial.training_scenario,
trial.training_scenario, trial.training_scenario, trial.training_scenario, trial.training_scenario,
trial.filler_scenario, trial.filler_scenario, trial.filler_scenario)]
</block>

<block finish>
/ trials = [
	1 = finish;
]
</block>

